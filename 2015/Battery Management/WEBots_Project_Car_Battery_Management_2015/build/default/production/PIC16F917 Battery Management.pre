void uint8ToASCII ( char* returnVal, uint8_t num )
{
uint8_t divisor = 100;
uint8_t prevSum = 0;
bool leading = 1;
uint8_t i = 0;


for (int j = 0; j <= 2; j++)
{
returnVal[j] = 0x00;
}

do
{


returnVal[i] = ( num - prevSum ) / divisor;

prevSum += returnVal[i]*divisor;

divisor = divisor/10;

if (leading == 1)
{
if ( returnVal[i] != 0 )
{

i++;
leading = 0;
}
}
else
i++;

} while ( divisor > 0);

i--;

for ( uint8_t k = 0; k <= i ; k++ )
{


returnVal[k] = returnVal[k] | 0x30;
}
}


void uint16ToASCII ( char* returnVal, uint16_t num )
