LCDWriteString(" V      ");


floatToASCII( temp2, cellVolt[3], 2);
LCDSetCursor(0x10);
LCDWriteString("Cell 4:");
LCDWriteString(temp2);
LCDWriteString(" V       ");

break;
}


case 3:
{

floatToASCII( temp1, cellVolt[4], 2);
LCDSetCursor(0x00);
LCDWriteString("Cell 5:");
LCDWriteString(temp1);
LCDWriteString(" V      ");


floatToASCII( temp2, cellVolt[5], 2);
LCDSetCursor(0x10);
LCDWriteString("Cell 6:");
LCDWriteString(temp2);
LCDWriteString(" V       ");

break;
}
}
}



void currentGainInit ( uint8_t gain )
{
if ( gain >= 100 )
{
RA6 = 1;

if ( gain == 100 )
{
RA7 = 0;
return;
}
else
{
RA7 = 1;
return;
}

}

else
{
RA6 = 0;

if ( gain == 25 )
{
RA7 = 0;
return;
}
else
{
RA7 = 1;
return;
}

}

}

void handlePB ()
{
if( RD5 == 1 )
return;

_delay((unsigned long)((50)*(8000000/4000.0)));

if( RD5 == 1 )
return;

stopWatch(0);
while( RD5 == 0 )
{
if(stopWatch(1) >= 1000)
{
initLCD();

RC1 = 1;
_delay((unsigned long)((100)*(8000000/4000.0)));
RC1 = 0;
return;
}
}

LCDDisplayMode++;

if (LCDDisplayMode > 3)
LCDDisplayMode = 0;
}

void checkCurrent ()
{
if ( current > 100)
{

_delay((unsigned long)((500)*(8000000/4000.0)));

sampleReference();
sampleCurrent();

if (current > 100)
{

RC0 = 0;
}
}
}




uint8_t systemCheck ()
{
sampleReference();
sampleCurrent();
sampleBatteryCells();


if ( cellVolt[0] < cellVoltL )
{

return 1;
}

return 0;
}
